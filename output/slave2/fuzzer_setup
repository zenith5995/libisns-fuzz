# environment variables:
AFL_AUTORESUME=1
AFL_CUSTOM_INFO_PROGRAM=./fuzz_simple_decode
AFL_CUSTOM_INFO_PROGRAM_ARGV=
AFL_CUSTOM_INFO_OUT=/output/slave2
AFL_I_DONT_CARE_ABOUT_MISSING_CRASHES=1
AFL_TRY_AFFINITY=1
AFL_SKIP_CPUFREQ=1
AFL_USE_ASAN=1
# command line:
'afl-fuzz' '-S' 'slave2' '-i' '-' '-o' '/output' './fuzz_simple_decode'
